{
  "name": "@mizu-mizu/array-matcher",
  "version": "1.1.3",
  "description": "Array matching utilities.",
  "main": "dist/array-matcher.js",
  "module": "dist/array-matcher.mjs",
  "jsnext:main": "dist/array-matcher.mjs",
  "author": "mizu-mizu",
  "license": "MIT",
  "repository": "github:uiui611/array-matcher",
  "scripts": {
    "test:unit": "mocha src/**/*.test.mjs --require @babel/register",
    "pretest:black-box": "npm run build",
    "test:black-box": "mocha examples/**/*.mjs --require @babel/register",
    "test": "npm-run-all -s test:unit test:black-box",
    "lint": "eslint src/**/*.mjs",
    "build": "rollup -c rollup.config.js",
    "jsdoc": "jsdoc src -r -d jsdoc -c jsdoc.json",
    "prepare": "npm-run-all -s build jsdoc",
    "prepublishOnly": "npm-run-all -s test lint"
  },
  "keywords": [
    "matcher",
    "match",
    "array",
    "es6",
    "glob"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^0.67.4"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs"
  ]
}
